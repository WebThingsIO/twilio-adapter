{
  "name": "twilio-adapter",
  "display_name": "Twilio",
  "version": "0.1.0",
  "description": "Simple Twilio API actions. Needs to be configured with an account SID, auth token, and Twilio-given number.",
  "main": "index.js",
  "scripts": {
    "lint": "eslint ."
  },
  "homepage": "https://github.com/mozilla-iot/twilio-adapter",
  "author": "Mozilla IoT",
  "license": "MPL-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/mozilla-iot/twilio-adapter.git"
  },
  "bugs": {
    "url": "https://github.com/mozilla-iot/twilio-adapter/issues"
  },
  "files": [
    "LICENSE",
    "SHA256SUMS",
    "index.js",
    "node_modules",
    "twilio-adapter.js",
    "twilio-notifier.js"
  ],
  "moziot": {
    "api": {
      "min": 2,
      "max": 2
    },
    "type": "notifier",
    "plugin": true,
    "exec": "{nodeLoader} {path}",
    "config": {
      "accountSid": "",
      "authToken": "",
      "fromPhoneNumber": "",
      "toPhoneNumber": ""
    },
    "schema": {
      "type": "object",
      "required": [
        "accountSid",
        "authToken",
        "fromPhoneNumber",
        "toPhoneNumber"
      ],
      "properties": {
        "accountSid": {
          "type": "string"
        },
        "authToken": {
          "type": "string"
        },
        "fromPhoneNumber": {
          "type": "string"
        },
        "toPhoneNumber": {
          "type": "string"
        }
      }
    }
  },
  "dependencies": {
    "twilio": "^3.33.0"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.2",
    "eslint": "^6.0.1"
  }
}
